{
    "name": "backend",
    "version": "1.0.0",
    "main": "./dist/server.js",
    "scripts": {
        "dev": "nodemon src/**/*.ts",
        "start": "node dist/server.js",
        "build": "tsc",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier . --write",
        "prepare": "husky",
        "precommit": "eslint ."
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@eslint/js": "^9.10.0",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^4.17.21",
        "@types/node": "^22.5.5",
        "eslint": "^9.10.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.5.0"
    },
    "dependencies": {
        "express": "^4.21.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "npm run lint:fix",
            "git add"
        ]
    }
}
